{
  "version": 2,
  "builds": [
    {
      "src": "client/dist/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "api-server-notion.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api-server-notion.js"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|svg|ico|json))",
      "dest": "/client/dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/client/dist/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "regions": ["sfo1"],
  "github": {
    "silent": true
  }
}